        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;(Refer to FAA-CT-8080-2H, Figure 2, Load Factor Chart) At what bank angle does the load factor of a fixed wing aircraft double the weight forces acting on the wings?&lt;/p&gt;
&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="8878">
                <material>
                  <mattext texttype="text/plain">15°</mattext>
                </material>
              </response_label>
              <response_label ident="6654">
                <material>
                  <mattext texttype="text/plain">30°</mattext>
                </material>
              </response_label>
              <response_label ident="3795">
                <material>
                  <mattext texttype="text/plain">45°</mattext>
                </material>
              </response_label>
              <response_label ident="1936">
                <material>
                  <mattext texttype="text/plain">60°</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">1936</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
