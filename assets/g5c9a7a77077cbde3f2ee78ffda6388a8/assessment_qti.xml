        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;strong&gt;Which is the correct traffic pattern departure procedure used by crewed aircraft at an airport?&lt;/strong&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="9365">
                <material>
                  <mattext texttype="text/plain">Depart in any direction consistent with safety, after crossing the airport boundary.</mattext>
                </material>
              </response_label>
              <response_label ident="6588">
                <material>
                  <mattext texttype="text/plain">Make all turns to the left.</mattext>
                </material>
              </response_label>
              <response_label ident="5317">
                <material>
                  <mattext texttype="text/plain">Comply with any FAA traffic pattern established for the airport and reported in the Chart Supplemental.</mattext>
