          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="1745">
                <material>
                  <mattext texttype="text/plain">To manage heavy commercial traffic.</mattext>
                </material>
              </response_label>
              <response_label ident="6455">
                <material>
                  <mattext texttype="text/plain">To contain all VFR flights.</mattext>
                </material>
              </response_label>
              <response_label ident="7113">
                <material>
                  <mattext texttype="text/plain">To provide a buffer zone around military installations.</mattext>
