        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;&lt;strong&gt;Scenario Set 1:&lt;/strong&gt;&lt;/p&gt;
&lt;p&gt;&lt;span&gt;You are planning an early afternoon real estate videography mission using a drone in the Phoenix area.&amp;nbsp;&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span style="font-family: 'courier new', courier;"&gt;METAR KPHX 111700Z 05005KT 10SM CLR 50/10 A2992 RMK AO2 SLP125&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span&gt;What does the "05005KT" in the METAR report indicate about the wind conditions at Phoenix Airport?&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="3357">
                <material>
                  <mattext texttype="text/plain">Wind is coming from 50 degrees at 5 knots.</mattext>
                </material>
              </response_label>
              <response_label ident="9980">
                <material>
                  <mattext texttype="text/plain">Wind is coming from 5 degrees at 50 knots.</mattext>
                </material>
              </response_label>
              <response_label ident="6880">
                <material>
                  <mattext texttype="text/plain">Wind is gusting up to 50 knots.</mattext>
                </material>
              </response_label>
              <response_label ident="1762">
                <material>
                  <mattext texttype="text/plain">Wind is variable with gusts up to 5 knots.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">3357</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
