    <section ident="root_section">
      <item ident="gf47b767d37e06559ff801f2d253307ba" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;&amp;nbsp;Is it allowed to fly a drone over people under Part 107?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="243">
                <material>
                  <mattext texttype="text/plain">Yes, at any time</mattext>
                </material>
              </response_label>
              <response_label ident="5906">
                <material>
                  <mattext texttype="text/plain">Only if they are directly participating in the operation.</mattext>
                </material>
              </response_label>
              <response_label ident="4747">
                <material>
                  <mattext texttype="text/plain">No, it is strictly prohibited.</mattext>
                </material>
              </response_label>
              <response_label ident="5276">
                <material>
                  <mattext texttype="text/plain">Yes, but only with special FAA permission.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">5906</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
      <item ident="gf47b767d37e06559ff801f2d253307ba" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;To conduct Category 1 operations, a remote pilot in command must use a small uncrewed aircraft that weighs:&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="6911">
                <material>
                  <mattext texttype="text/plain">0.75 pounds or less</mattext>
                </material>
              </response_label>
              <response_label ident="4703">
                <material>
                  <mattext texttype="text/plain">0.65 pounds or less</mattext>
                </material>
              </response_label>
              <response_label ident="181">
                <material>
                  <mattext texttype="text/plain">0.55 pounds or less</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">181</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <other/>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_incorrect_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/plain">Correct. According to ยง 107.110, the uncrewed aircraft in use must weigh 0.55 pounds or less on takeoff and throughout the duration of each operation under Category 1, including everything that is on board or otherwise attached to the aircraft.</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="general_incorrect_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/plain">Incorrect. According to ยง 107.110, the uncrewed aircraft in use must weigh 0.55 pounds or less on takeoff and throughout the duration of each operation under Category 1, including everything that is on board or otherwise attached to the aircraft.</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
