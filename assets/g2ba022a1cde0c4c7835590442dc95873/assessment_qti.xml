    <section ident="root_section">
      <item ident="gf47b767d37e06559ff801f2d253307ba" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;What is the cost to register a drone under Part 107?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="4234">
                <material>
                  <mattext texttype="text/plain">FREE</mattext>
                </material>
              </response_label>
              <response_label ident="4386">
                <material>
                  <mattext texttype="text/plain">$2</mattext>
                </material>
              </response_label>
              <response_label ident="6994">
                <material>
                  <mattext texttype="text/plain">$5</mattext>
                </material>
              </response_label>
              <response_label ident="9528">
                <material>
                  <mattext texttype="text/plain">$20</mattext>
                </material>
              </response_label>
              <response_label ident="2322">
                <material>
                  <mattext texttype="text/plain">It depends on the size of the drone.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">6994</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
      <item ident="gf47b767d37e06559ff801f2d253307ba" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;How long is the registration period for a drone under Part 107?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="8273">
                <material>
                  <mattext texttype="text/plain">2 years</mattext>
                </material>
              </response_label>
              <response_label ident="1445">
                <material>
                  <mattext texttype="text/plain">3 years</mattext>
                </material>
              </response_label>
              <response_label ident="7638">
                <material>
                  <mattext texttype="text/plain">5 years</mattext>
                </material>
              </response_label>
              <response_label ident="2333">
                <material>
                  <mattext texttype="text/plain">For the life of the drone.</mattext>
                </material>
              </response_label>
              <response_label ident="812">
                <material>
                  <mattext texttype="text/plain">Drones don't need to be registered under Part 107.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">1445</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="1445_fb"/>
          </respcondition>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">1445</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <other/>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_incorrect_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">That's right! &lt;span&gt;Under Part 107, the registration of a drone is valid for a period of 3 years. After this period, the registration must be renewed.&amp;nbsp;&lt;/span&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="general_incorrect_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">Under Part 107, the registration of a drone is valid for a period of 3 years. After this period, the registration must be renewed.&amp;nbsp;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="1445_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&amp;nbsp;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
