              </response_label>
              <response_label ident="144">
                <material>
                  <mattext texttype="text/plain">700 feet AGL</mattext>
                </material>
              </response_label>
              <response_label ident="3705">
                <material>
                  <mattext texttype="text/plain">1,200 feet AGL</mattext>
                </material>
              </response_label>
              <response_label ident="3479">
