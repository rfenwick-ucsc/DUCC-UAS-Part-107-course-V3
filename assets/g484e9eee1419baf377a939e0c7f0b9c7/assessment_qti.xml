        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;strong&gt;Which of the following are clues that a temperature inversion might exist?&lt;/strong&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="7206">
                <material>
                  <mattext texttype="text/plain">Light and variable winds (&lt; 3 mph)</mattext>
                </material>
              </response_label>
              <response_label ident="3232">
                <material>
                  <mattext texttype="text/plain">Low elevation areas such as valleys and basins where cool air can sink and collect</mattext>
                </material>
              </response_label>
              <response_label ident="1450">
                <material>
                  <mattext texttype="text/plain">Mist, fog, dew or frost are present with mostly clear skies above.</mattext>
                </material>
              </response_label>
              <response_label ident="1754">
                <material>
                  <mattext texttype="text/plain">All are factors that suggest a temperature inversion might exist</mattext>
                </material>
              </response_label>
              <response_label ident="3369">
                <material>
                  <mattext texttype="text/plain">None of these are factors that suggest a temperature inversion might exist.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">1754</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
