      <item ident="gf47b767d37e06559ff801f2d253307ba" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;When a control tower, located on an airport within Class D airspace, ceases operation for the day, what happens to the airspace designation?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="5029">
                <material>
                  <mattext texttype="text/plain">The airspace designation normally will not change.</mattext>
                </material>
              </response_label>
              <response_label ident="3570">
                <material>
                  <mattext texttype="text/plain">The airspace reverts to Class E or a combination of Class E and G airspace during the hours the tower is not in operation.</mattext>
                </material>
              </response_label>
              <response_label ident="9264">
                <material>
                  <mattext texttype="text/plain">The airspace remains Class D airspace as long as the weather observer or automated weather system is available.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">3570</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
      <item ident="gf47b767d37e06559ff801f2d253307ba" title="Question">
