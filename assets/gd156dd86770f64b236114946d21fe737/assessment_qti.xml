        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;What is the relationship of lift, drag, thrust, and weight when the airplane is in straight-and-level flight?&lt;/p&gt;
&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="5740">
                <material>
                  <mattext texttype="text/plain">Lift equals weight and thrust equals drag.</mattext>
                </material>
              </response_label>
              <response_label ident="7181">
                <material>
                  <mattext texttype="text/plain">Lift, drag, and weight equal thrust.</mattext>
                </material>
              </response_label>
              <response_label ident="3105">
                <material>
                  <mattext texttype="text/plain">Lift and weight equal thrust and drag.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
