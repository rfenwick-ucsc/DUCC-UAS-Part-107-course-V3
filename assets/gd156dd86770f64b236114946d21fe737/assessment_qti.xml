        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;What are the four forces action on a drone in flight?&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="5450">
                <material>
                  <mattext texttype="text/plain">weight, thrust, power, and drag.</mattext>
                </material>
              </response_label>
              <response_label ident="3271">
                <material>
                  <mattext texttype="text/plain">pitch, roll, yaw, and thrust.</mattext>
                </material>
              </response_label>
              <response_label ident="9627">
                <material>
                  <mattext texttype="text/plain">thrust, drag, weight (gravity), and lift.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">9627</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
