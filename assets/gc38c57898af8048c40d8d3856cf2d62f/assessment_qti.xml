          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;strong&gt;What are some challenges that may be encountered when flying a drone in freezing temperatures?&lt;/strong&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="3196">
                <material>
                  <mattext texttype="text/plain">Lower battery capacity on transmitter, monitor, aircraft power, etc.</mattext>
                </material>
              </response_label>
              <response_label ident="9307">
                <material>
                  <mattext texttype="text/plain">Poor sensitivity on the control sticks because of numb fingers</mattext>
                </material>
              </response_label>
              <response_label ident="1643">
                <material>
                  <mattext texttype="text/plain">Increased distraction on the pilot.</mattext>
                </material>
              </response_label>
              <response_label ident="6640">
                <material>
                  <mattext texttype="text/plain">Potential for icing of wings or propellers.</mattext>
                </material>
              </response_label>
              <response_label ident="8123">
                <material>
                  <mattext texttype="text/plain">All are possible challenges in freezing temperatures.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
