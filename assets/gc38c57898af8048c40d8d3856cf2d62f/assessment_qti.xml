        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;strong&gt;Why is frost considered hazardous to sUAS operations?&lt;/strong&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="9723">
                <material>
                  <mattext texttype="text/plain">Frost decreases lift capacity by changing the aerodynamic shape of the propellers.</mattext>
                </material>
              </response_label>
              <response_label ident="5687">
                <material>
                  <mattext texttype="text/plain">Frost decreases lift capacity by spoiling the smooth flow of air over the propellers.</mattext>
                </material>
              </response_label>
              <response_label ident="8648">
                <material>
                  <mattext texttype="text/plain">Frost increases control effectiveness by slowing the airflow.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">5687</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
