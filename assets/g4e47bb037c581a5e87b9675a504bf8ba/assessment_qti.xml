            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;Is it permissible to operate a drone from a moving vehicle under Part 107?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="7527">
                <material>
                  <mattext texttype="text/plain">Yes, in any situation</mattext>
                </material>
              </response_label>
              <response_label ident="4864">
                <material>
                  <mattext texttype="text/plain">Yes, but only in urban areas</mattext>
                </material>
              </response_label>
              <response_label ident="5675">
                <material>
                  <mattext texttype="text/plain">Yes, over sparsely populated areas and not for transporting property for compensation</mattext>
                </material>
              </response_label>
              <response_label ident="6401">
                <material>
                  <mattext texttype="text/plain">No, it's never permitted</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">5675</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
