        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;&lt;span&gt;(Refer to FAA-CT-8080-2H, Figure 23) If you are inspecting the high-intensity lights on top of the towers 5 1/2 nautical miles southwest of Savannah International Airport, would you need prior authorization from ATC?&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="5004">
                <material>
                  <mattext texttype="text/plain">No, because you’d be under the shelf of Class C airspace</mattext>
                </material>
              </response_label>
              <response_label ident="1616">
                <material>
                  <mattext texttype="text/plain">Yes, because you’d be operating in Class C airspace</mattext>
                </material>
              </response_label>
              <response_label ident="4025">
                <material>
                  <mattext texttype="text/plain">Yes, because you’d be operating in Class B airspace</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">5004</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="5004_fb"/>
