            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;In aviation, what condition defines the ‘ceiling’&amp;nbsp; is defined as:&amp;nbsp;&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="3815">
                <material>
                  <mattext texttype="text/plain">The lowest broken or overcast layer or vertical visibility into an obscuration (e.g., fog or haze).</mattext>
                </material>
              </response_label>
              <response_label ident="6713">
                <material>
                  <mattext texttype="text/plain">The lowest layer of clouds reported as scattered, broken, or thin.</mattext>
                </material>
              </response_label>
              <response_label ident="6993">
                <material>
                  <mattext texttype="text/plain">The lowest reported obscuration and the highest layer of clouds reported as overcast.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">3815</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
