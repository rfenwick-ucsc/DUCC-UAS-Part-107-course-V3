            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;&amp;nbsp;If a drone pilot wishes to fly in an area where temporary flight restrictions (TFRs) are in place, what is required?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="9916">
                <material>
                  <mattext texttype="text/plain">No special permission is required for drones.</mattext>
                </material>
              </response_label>
              <response_label ident="8121">
                <material>
                  <mattext texttype="text/plain">The pilot must notify the nearest airport.</mattext>
                </material>
              </response_label>
              <response_label ident="7810">
                <material>
                  <mattext texttype="text/plain">Special authorization from the FAA is required.</mattext>
                </material>
              </response_label>
              <response_label ident="5014">
                <material>
                  <mattext texttype="text/plain">The pilot must have a commercial pilot license.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">7810</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
