        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;&lt;span&gt;In which incident would a Remote PIC report an accident?&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="7314">
                <material>
                  <mattext texttype="text/plain">$600 in damage to a fence that has a fair market value of $200</mattext>
                </material>
              </response_label>
              <response_label ident="9382">
                <material>
                  <mattext texttype="text/plain">$700 in damage to a car that has a fair market value of $14,000</mattext>
                </material>
              </response_label>
              <response_label ident="9414">
                <material>
                  <mattext texttype="text/plain">The $1,200 drone is totaled</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">9382</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
      <item ident="gf47b767d37e06559ff801f2d253307ba" title="Question">
