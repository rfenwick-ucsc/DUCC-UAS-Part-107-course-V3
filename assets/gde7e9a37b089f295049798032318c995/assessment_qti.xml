        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;A small UA causes an accident and your crew member loses consciousness. When do you report the accident?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="4072">
                <material>
                  <mattext texttype="text/plain">No accidents need to be reported.</mattext>
                </material>
              </response_label>
              <response_label ident="122">
                <material>
                  <mattext texttype="text/plain">When requested by the UA owner.</mattext>
                </material>
              </response_label>
              <response_label ident="6902">
                <material>
                  <mattext texttype="text/plain">Within 10 days of the accident.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
