        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;What is generally the vertical limit of Class B airspace?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="7039">
                <material>
                  <mattext texttype="text/plain">10,000 feet MSL</mattext>
                </material>
              </response_label>
              <response_label ident="4810">
                <material>
                  <mattext texttype="text/plain">Up to but not including 18,000 feet MSL</mattext>
                </material>
              </response_label>
              <response_label ident="2097">
                <material>
                  <mattext texttype="text/plain">Surface to 4,000 feet above the airport elevation</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
