              </response_label>
              <response_label ident="9756">
                <material>
                  <mattext texttype="text/plain">Class B</mattext>
                </material>
              </response_label>
              <response_label ident="4469">
                <material>
                  <mattext texttype="text/plain">Class D</mattext>
                </material>
              </response_label>
              <response_label ident="8281">
                <material>
                  <mattext texttype="text/plain">Class A</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
