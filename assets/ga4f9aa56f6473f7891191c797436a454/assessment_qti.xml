        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;Which of the following must the remote pilot in command assess before conducting a drone operation?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="8074">
                <material>
                  <mattext texttype="text/plain">Local weather conditions.</mattext>
                </material>
              </response_label>
              <response_label ident="746">
                <material>
                  <mattext texttype="text/plain">Local airspace and any flight restrictions.</mattext>
                </material>
              </response_label>
              <response_label ident="8485">
                <material>
                  <mattext texttype="text/plain">The location of persons and property on the surface.</mattext>
                </material>
              </response_label>
              <response_label ident="6271">
                <material>
                  <mattext texttype="text/plain">Identify potential ground hazards.</mattext>
                </material>
              </response_label>
              <response_label ident="9851">
