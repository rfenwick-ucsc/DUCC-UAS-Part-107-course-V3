        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;Which of the following are required before flying a drone under Part 107?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Multiple">
            <render_choice>
              <response_label ident="7873">
                <material>
                  <mattext texttype="text/plain">Notify the nearest airport</mattext>
                </material>
              </response_label>
              <response_label ident="8965">
                <material>
                  <mattext texttype="text/plain">Perform a preflight inspection</mattext>
                </material>
              </response_label>
              <response_label ident="6770">
                <material>
                  <mattext texttype="text/plain">Obtain written permission from local authorities</mattext>
                </material>
              </response_label>
              <response_label ident="5356">
                <material>
                  <mattext texttype="text/plain">Conduct a test flight in a controlled environment</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <and>
                <not>
                  <varequal respident="response1">7873</varequal>
                </not>
                <varequal respident="response1">8965</varequal>
                <not>
                  <varequal respident="response1">6770</varequal>
                </not>
                <not>
                  <varequal respident="response1">5356</varequal>
                </not>
              </and>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
